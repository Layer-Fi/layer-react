{
  "name": "@layerfi/components",
  "version": "0.1.123-alpha",
  "description": "Layer React Components",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./index.css": "./dist/index.css",
    "./dist/styles/index.css": "./dist/index.css"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "vite build --config vite/vite.config.ts --mode esm && vite build --config vite/vite.config.ts --mode cjs",
    "build:clean": "rm -rf ./dist && npm run build",
    "dev:js": "vite build --config vite/vite.config.ts --mode esm --watch",
    "dev:types": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && tsc -p tsconfig.build.json --watch --preserveWatchOutput & chokidar 'dist/**/*.d.ts' --silent -c 'tsc-alias -p tsconfig.build.json'",
    "dev": "npm run dev:js & npm run dev:types",
    "prepack": "npm run typecheck && npm run build:clean",
    "lint": "eslint . && stylelint '**/*.{css,scss}'",
    "lint:fix": "eslint . --fix && stylelint '**/*.{css,scss}' --fix",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint '**/*.{css,scss}'",
    "clear:react": "rm -rf ./node_modules/react && rm -rf ./node_modules/react-dom",
    "prepare": "husky",
    "migrate:aliases": "tsx scripts/migrate-import-aliases.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Layer-Fi/layer-react.git"
  },
  "keywords": [
    "layerfi",
    "accounting",
    "react"
  ],
  "author": "Layer Financial",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/Layer-Fi/layer-react/issues"
  },
  "homepage": "https://github.com/Layer-Fi/layer-react#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.9.0",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "npm-dts": "^1.3.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "ts-morph": "^27.0.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{css,scss}": "stylelint --fix"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.8",
    "@internationalized/date": "^3.8.2",
    "@react-hook/resize-observer": "^2.0.2",
    "@tanstack/react-form": "^1.19.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "calendar-link": "^2.11.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "effect": "^3.16.3",
    "libphonenumber-js": "^1.12.7",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.507.0",
    "motion": "^12.23.11",
    "pluralize": "^8.0.0",
    "react-aria-components": "^1.13.0",
    "react-calendly": "^4.4.0",
    "react-currency-input-field": "^3.10.0",
    "react-dropzone": "^14.3.8",
    "react-merge-refs": "^3.0.2",
    "react-plaid-link": "^4.0.1",
    "react-select": "^5.10.1",
    "recharts": "^3.6.0",
    "swr": "^2.3.3",
    "ts-ics": "^2.4.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.4"
  }
}